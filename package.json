{
  "name": "llm-benchmarks",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build:frontend && wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "build:frontend": "vite build",
    "db:migrate": "wrangler d1 execute llm-benchmarks-db --remote --file=./src/db/schema.sql",
    "db:migrate:local": "wrangler d1 execute llm-benchmarks-db --local --file=./src/db/schema.sql"
  },
  "dependencies": {
    "hono": "^4.11.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260207.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^27.4.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.0",
    "vitest": "^3.0.4",
    "wrangler": "^4.63.0"
  }
}
